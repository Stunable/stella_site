[group:{{app_name}}]
programs=gunicorn,celery

[program:gunicorn]
command={{project_root}}/bin/python manage.py run_gunicorn -c gunicorn.conf.py -p gunicorn.pid
directory={{manage_path}}
user={{project_user}}
autostart=true
autorestart=true
redirect_stderr=true

[program:celery]
command={{project_root}}/bin/python manage.py celeryd -c 1 --settings celery_settings
directory={{manage_path}}
user={{project_user}}
autostart=true
autorestart=true
redirect_stderr=true